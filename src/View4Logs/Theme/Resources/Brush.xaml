<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:theme="clr-namespace:View4Logs.Theme">
    <SolidColorBrush
        x:Key="{x:Static theme:Brush.AppBarBackground}"
        Color="{x:Static theme:Color.AppBarBackground}" />
    <SolidColorBrush
        x:Key="{x:Static theme:Brush.Background}"
        Color="{x:Static theme:Color.Background}" />
    <SolidColorBrush
        x:Key="{x:Static theme:Brush.Border}"
        Color="{x:Static theme:Color.Border}" />
    <SolidColorBrush
        x:Key="{x:Static theme:Brush.HintText}"
        Color="{x:Static theme:Color.HintText}" />
    <SolidColorBrush
        x:Key="{x:Static theme:Brush.Text}"
        Color="{x:Static theme:Color.Text}" />
</ResourceDictionary>
