<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:theme="clr-namespace:View4Logs.UI.Theme">

    <sys:Double x:Key="{x:Static theme:FontSize.ExtraSmall}">6</sys:Double>
    <sys:Double x:Key="{x:Static theme:FontSize.Small}">12</sys:Double>
    <sys:Double x:Key="{x:Static theme:FontSize.Normal}">18</sys:Double>
    <sys:Double x:Key="{x:Static theme:FontSize.Large}">24</sys:Double>
    <sys:Double x:Key="{x:Static theme:FontSize.ExtraLarge}">32</sys:Double>

</ResourceDictionary>
